{
  "version": 2,
  "outputCapture": "direct-nodejs",
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "title": "Browser executor",
  "description": "",
  "type": "object",
  "properties": {
    "bundleAnalyzer": {
      "type": "boolean",
      "description": "Visualize size of webpack output files with an interactive zoomable treemap.",
      "default": false
    },
    "outputPath": {
      "type": "string",
      "description": "The full path for the new output directory, relative to the current workspace."
    },
    "minify": {
      "type": "boolean",
      "description": "Build website minimizing JS bundles.",
      "default": true
    }
  },
  "required": ["outputPath"]
}
