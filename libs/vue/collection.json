{
  "$schema": "../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "name": "vue",
  "version": "0.0.1",
  "schematics": {
    "application": {
      "factory": "./src/schematics/application/schematic",
      "schema": "./src/schematics/application/schema.json",
      "description": "application schematic",
      "aliases": ["app"]
    },
    "vuex": {
      "factory": "./src/schematics/vuex/schematic",
      "schema": "./src/schematics/vuex/schema.json",
      "description": "vuex schematic"
    },
    "library": {
      "factory": "./src/schematics/library/schematic",
      "schema": "./src/schematics/library/schema.json",
      "description": "library schematic",
      "aliases": ["lib"]
    },
    "component": {
      "factory": "./src/schematics/component/schematic",
      "schema": "./src/schematics/component/schema.json",
      "description": "Create a component",
      "aliases": ["c"]
    }
  }
}
