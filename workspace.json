            "outputPath": "dist/libs/nuxt",
            "tsConfig": "libs/nuxt/tsconfig.lib.json",
            "packageJson": "libs/nuxt/package.json",
            "main": "libs/nuxt/src/index.ts",
            "assets": [
              "libs/nuxt/*.md",
              {
                "input": "./libs/nuxt/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
